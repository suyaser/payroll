{
  "name": "castle-dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "build": "babel src/server --out-dir dist/server",
    "d-client": "parcel src/client/index.html --port 3000",
    "client": "SET NODE_ENV=production& parcel src/client/index.html --port 3000",
    "d-server": "nodemon ./src/server/www.js --exec babel-node",
    "server": "npm run build && SET NODE_ENV=production& node ./dist/server/www.js",
    "dev": "concurrently \"npm run d-server\" \"npm run d-client\"",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "@coreui/coreui": "^2.0.6",
    "@coreui/icons": "^0.3.0",
    "@coreui/react": "^2.0.5",
    "@reach/router": "^1.1.1",
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "concurrently": "^4.0.1",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.0",
    "debug": "~2.6.9",
    "export-to-csv": "^0.2.1",
    "express": "~4.16.0",
    "flag-icon-css": "^3.0.0",
    "helmet": "^3.13.0",
    "http-errors": "~1.6.2",
    "install": "^0.12.2",
    "kue": "^0.11.6",
    "match-sorter": "^2.3.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "node-cmd": "^3.0.0",
    "node-datetime": "^2.1.0",
    "node-svn-ultimate": "^1.1.0",
    "npm": "^6.4.1",
    "parcel": "^1.11.0",
    "pretty-ms": "^3.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-loadable": "^5.5.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-table": "^6.8.6",
    "react-toastify": "^4.5.2",
    "reactstrap": "^6.4.0",
